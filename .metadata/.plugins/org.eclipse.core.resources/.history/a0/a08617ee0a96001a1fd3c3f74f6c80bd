public class Teste {

	public static void main(String[] args) {

		int idades[] = new int[5];

		int index = 0;

		for (int v = 16; v < 16 + 4; v++) {
			idades[index] = v;
			index++;
		}
		
		System.out.println(idades[0]);
	}
}

